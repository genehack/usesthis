{
  "rewrites": [
    {
      "source": "/",
      "destination": "/interviews/index.html"
    },
    {
      "source": "/api/:path",
      "has": [
        {
          "type": "query",
          "key": "page",
          "value": "(?<page>\\d+)"
        }
      ],
      "destination": "/api/:path/page_2.json"
    }
  ]
}
